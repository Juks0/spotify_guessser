    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "lucide-react": "^0.539.0",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "querystring": "^0.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "request": "^2.88.2",
    "socket.io": "^4.8.1",
