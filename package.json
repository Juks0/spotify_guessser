{
  "name": "spotify_guess",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run db:check\"",
    "dev:frontend": "vite --host",
    "dev:backend": "concurrently \"npm:dev:backend-auth\" \"npm:dev:backend-socket\"",
    "dev:backend-auth": "tsx src/lib/auth/authorize.ts",
    "dev:backend-socket": "tsx src/lib/server/server.ts",
    "db:check": "tsx src/scripts/check-database.ts",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
